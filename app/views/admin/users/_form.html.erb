    <div id="Box_register_form">	
		<%= form_for [:admin, @user] do |f| %>
			<%= f.error_messages %>
			<p>
				<%= f.label :nome, "Nome completo:" %>
	            <%=f.text_field :nome%>
			</p>
			<p>
				<%= f.label :email, "Email:"%>
		        <%=f.text_field :email%>
			</p>
			<p>
				<%= f.label :cpf, "Cpf:"%>
		        <%=f.text_field :cpf%>
			</p>
			<p>
				<%= f.label :rg, "Rg:"%>
		        <%=f.text_field :rg%>
			</p>
			<p>
				<%= f.label :city, "Cidade:"%>
		        <%=f.text_field :city%>
			</p>
			<p>
				<%= f.label :state, "Uf:", :style=>" vertical-align:middle;" %>
				<%= f.select :state, ([["AC", "AC"], ["AL","AL"], ["AP", "AP"],
				["AM", "AM"], ["BA", "BA"], ["CE", "CE"],
				["DF", "DF"], ["ES", "ES"], ["GO", "GO"],
				["MA", "MA"], ["MT", "MT"], ["MS", "MS"],
				["MG", "MG"], ["PA", "PA"], ["PB", "PB"],
				["PR", "PR"], ["PE", "PE"], ["PI", "PI"],
				["RJ", "RJ"], ["RN", "RN"], ["RS", "RS"],
				["RO", "RO"], ["RR", "RR"], ["SC", "SC"],
				["SP", "SP"], ["SE", "SE"], ["TO", "TO"]]) %>
			</p>
			<p>
				<%= f.label :password, "Senha:" %>
				<%=f.password_field :password%>
			</p>
			<p>
				<%= f.label :password_confirmation, "Repita Senha:" %>
			 	<%=f.password_field :password_confirmation%>
			</p>
		  <h1>Regras</h1>
		  <p>
			Tipo:<br />
			<%= f.radio_button :role, :admin%>Administrador
			<%= f.radio_button :role, :supervisor %>Supervisor
			<%= f.radio_button :role, :seller %>Usu√°rio
		  </p><br />
  		<p><%= f.submit %></p>
		<%end%>
	</div>
