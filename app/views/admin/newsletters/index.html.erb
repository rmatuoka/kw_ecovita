<% title "Admin/Newsletters" %>

<table>
  <tr>
    <th>Email</th>
  </tr>
  <% for newsletter in @newsletters %>
    <tr>
      <td><%= newsletter.email %></td>
      <td><%= link_to "Show", [:admin, newsletter] %></td>
      <td><%= link_to "Edit", edit_admin_newsletter_path(newsletter) %></td>
      <td><%= link_to "Destroy", [:admin, newsletter], :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Admin/Newsletter", new_admin_newsletter_path %></p>
