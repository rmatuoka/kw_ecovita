<% title "Page" %>

<p>
  <strong>Name:</strong>
  <%= @page.name %>
</p>
<p>
  <strong>Page Title:</strong>
  <%= @page.page_title %>
</p>
<p>
  <strong>Body:</strong>
  <%= @page.body %>
</p>
<p>
  <strong>Published:</strong>
  <%= @page.published %>
</p>

<p>
  <%= link_to "Edit", edit_admin_page_path(@page) %> |
  <%= link_to "Destroy", admin_page_path(@page), :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", admin_pages_path %>
</p>
